package com.educacionit.jwa.backend.backend.jwa.models;

import javax.persistence.Entity;
import javax.persistence.Table;

@Entity
@Table(name = "directors")
public class DirectorModel extends PersonModel {

	private String movies;
	/*
	@OneToMany(cascade = CascadeType.ALL,fetch = FetchType.EAGER)
	@JoinColumn(name = "productions")
	private List<FilmModel> productions;
	*/
	
	public DirectorModel() {
	}

	public DirectorModel(String name, String lastname, Integer id, String movies) {
		super(id, name, lastname);
		this.movies = movies;
	}

	public  String getmovies() {
		return movies;
	}

	public void setMovies(String movies) {
		this.movies = movies;
	}

}
