package com.educacionit.jwa.backend.backend.jwa.models;

import java.util.ArrayList;
import java.util.List;

import javax.persistence.Entity;
import javax.persistence.Table;

@Entity
@Table(name = "directors")
public class DirectorModel extends PersonModel {

	private FilmModel production;

	public DirectorModel() {
	}

	public DirectorModel(Integer id, String name, String lastname, FilmModel production) {
		super(id, name, lastname);
		this.production = production;
	}

	public FilmModel getProduction() {
		return production;
	}

	public void setProductions(FilmModel production) {
		this.production = production;
	}

	
}
