package com.educacionit.jwa.backend.backend.jwa.repositories;

import java.util.List;

import org.springframework.data.repository.CrudRepository;
import org.springframework.stereotype.Repository;

import com.educacionit.jwa.backend.backend.jwa.models.DirectorModel;

@Repository
public interface DirectorDAO extends CrudRepository<DirectorModel, String>{
	public List<DirectorModel>findALL();
	public DirectorModel findById(String id);
}
